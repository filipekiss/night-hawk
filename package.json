        "info:generate": "ts-node src/scripts/generate-info.ts",
        "vim:generate": "ts-node src/scripts/generate-vim-files.ts",
        "build": "npm run docs:build && npm run info:generate"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/filipekiss/night-hawk.git"
    },
