    "bugs": {
        "url": "https://github.com/filipekiss/night-hawk/issues"
    },
    "homepage": "https://github.com/filipekiss/night-hawk#readme",
    "devDependencies": {
        "@types/nunjucks": "^3.1.1",
        "estilo": "^1.3.3",
        "nunjucks": "^3.2.0",
        "parcel": "^1.12.3",
        "sass": "^1.18.0",
        "ts-node": "^8.0.3",
        "typescript": "^3.4.3",
