{
    "name": "@filipekiss/nighthawk",
    "version": "0.0.0",
    "description": "Vim Colorscheme",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "docs:dev": "parcel src/docs/index.html",
        "docs:build": "parcel build src/docs/index.html --out-dir docs --public-url /night-hawk/",
